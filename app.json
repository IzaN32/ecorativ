{
  "expo": {
    "name": "ecorativ",
    "slug": "ecorativ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/appicon.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "##0f5728"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon.jpeg",
        "backgroundColor": "#0f5728"
      },
      "package": "com.hahahihi.ecorativ"
    },
    "web": {
      "favicon": "./assets/appicon.jpeg"
    },
    "extra": {
      "eas": {
        "projectId": "b10cb717-50f6-45c5-a360-d51a371ba926"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
